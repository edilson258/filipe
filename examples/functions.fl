define sayHelloTo(name: string): null {
  print("Hello ", name);
}

sayHelloTo("World!");
sayHelloTo("Edilson");

define concat(lhs: string, rhs: string): string {
  return lhs + rhs;
}

define sum(x: number, y: number): number {
  let res = x + y;
  return res;
}

let foo = 34;
let bar: number = 35;

print(foo, " + ", bar, " = ", sum(foo, bar));
print("'foo' + 'bar' = '", concat("foo", "bar"), "'");

print(10-- * 10);
